{
  "manifest_version": 3,
  "name": "Web Utility Plugin",
  "description": "This extension will fix some anoying things about multiple websites",
  "version": "1.0.42",
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "all_frames": true,
      "run_at": "document_start",
      "js": [
        "utility.js"
      ]
    }
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  }
}
